{
  "package": "com.vivo.cardDemo",
  "name": "card-demo",
  "versionName": "1.0.0",
  "versionCode": "1",
  "minPlatformVersion": "101",
  "icon": "/Common/logo.png",
  "features": [
    {
      "name": "system.prompt"
    },
    {
      "name": "system.router"
    },
    {
      "name": "system.shortcut"
    }
  ],
  "permissions": [
    {
      "origin": "*"
    }
  ],
  "config": {                                               
    "logLevel": "debug",
    "designWidth": 750
  },
  "router": {
    "widgets": {
      "Card": {
        "name": "Card",
        "description": "快应用卡片展示",
        "component": "index",
        "path": "/Card",
        "features": [
          {
            "name": "system.router"
          }
        ],
        "targetManufactorys": [
          "vivo"
        ]
      }
    }
  },
  "display": {
    "titleBarBackgroundColor": "#f2f2f2",
    "titleBarTextColor": "#414141",
    "menu": true,
    "pages": {
      "Demo": {
        "titleBarText": "示例页",
        "menu": false
      }
    }
  }
}